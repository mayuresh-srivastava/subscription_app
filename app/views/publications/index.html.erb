<h2>Publications</h2>

<% @publications.each do |x| %>
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title"><%= x.title %></h3>
    </div>
    <div class="panel-body">
      <p><%= x.description %></p>
      <p><%= x.file_url %></p>
    </div>
    <div class="panel-footer">
      <%= link_to "View more details", publication_path(x), class: "btn btn-default" %>
    </div>
  </div>
<% end %>



